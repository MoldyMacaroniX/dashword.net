    {% if pageID == "authorPosts" %}{% set myAuthor = authors | getAuthor(category.title) %}{% endif %}
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    {% if page404 %}
    <!-- If 404 -->
    <meta name="robots" content="noindex,follow">
    {% endif %}

    <meta name="theme-color" content="#0067EE">
    <link rel="icon" href="/assets/img/icon.png">

    <!-- Metadata -->
    <title>
    {% if pageID !== "authorPosts" %}{{ title or metadata.title }}{% else %}{{ myAuthor.name }}{% endif %} |{% if course %} {{ coursename }}, a course on{% endif %} {{ metadata.title }}
    </title>
    <meta name="title" content="{% if pageID !== "authorPosts" %}{{ title or metadata.title }}{% else %}{{ myAuthor.name }}{% endif %} | {{ metadata.title }}">
    <meta name="description" content="{% if pageID !== "authorPosts" %}{{ desc or metadata.desc }}{% else %}{{ myAuthor.bio }}{% endif %}">
    {% if date %}<meta name="date" content="{{ page.date | readableDate }}">{% endif %}

    <!-- MAIN CSS -->
    <link rel="stylesheet" href="/assets/css/index.css">

    <!-- ICON CSS -->
    <link href="https://cdn.jsdelivr.net/npm/remixicon@3.5.0/fonts/remixicon.css" rel="stylesheet">

    <!-- Toastify JS -->
    <link rel="stylesheet" type="text/css" href="https://cdn.jsdelivr.net/npm/toastify-js/src/toastify.min.css">

    <!-- Tingle -->
    <link rel="stylesheet" type="text/css" href="/assets/css/tingle.css">

    <!-- Opengraph -->
    <meta property="og:type" content="website">
    <meta property="og:url" content="{{ metadata.url }}">
    <meta property="og:title" content="{% if pageID !== "authorPosts" %}{{ title or metadata.title }}{% else %}{{ myAuthor.name }}{% endif %} |{% if course %} {{ coursename }}, a course on{% endif %} {{ metadata.title }}">
    <meta property="og:description" content="{% if pageID !== "authorPosts" %}{{ desc or metadata.desc }}{% else %}{{ myAuthor.bio }}{% endif %}">
    <meta property="og:image" content="{{ image or metadata.image }}">

    <!-- Twitter since they're different or something -->
    <meta property="twitter:card" content="summary_large_image">
    <meta property="twitter:url" content="{{ metadata.url }}">
    <meta property="twitter:title" content="{% if pageID !== "authorPosts" %}{{ title or metadata.title }}{% else %}{{ myAuthor.name }}{% endif %} |{% if course %} {{ coursename }}, a course on{% endif %} {{ metadata.title }}">
    <meta property="twitter:description" content="{% if pageID !== "authorPosts" %}{{ desc or metadata.desc }}{% else %}{{ myAuthor.bio }}{% endif %}">
    <meta property="twitter:image" content="{{ image or metadata.image }}">

    <!-- Netlify CMS -->
    <script src="https://identity.netlify.com/v1/netlify-identity-widget.js"></script>

    <!-- Barn.js (for easy working with localstorage) -->
    <script type="text/javascript" src="/assets/js/barn.js"></script>

</head>
<body onmouseleave="beforeLeave();" id="body">

    <script>
        window.onload = async function() {
            console.log("Detected Theme: " + barn.get("theme"));
            if (barn.get("theme") == "system") {
                setThemeDevice();
            }
            if (barn.get("theme") == "light") {
                setThemeLight();
            }
            if (barn.get("theme") == "dark") {
                setThemeDark();
            }
        }
    </script>

    <script>
        var bod = document.getElementById("body");
        async function setThemeDevice() {
            barn.set("theme", "system");
            body.classList.remove("light");
            body.classList.remove("dark");
        }
        async function setThemeLight() {
            barn.set("theme", "light");
            body.classList.add("light");
            body.classList.remove("dark");
        }
        async function setThemeDark() {
            barn.set("theme", "dark");
            body.classList.add("dark");
            body.classList.remove("light");
        }
    </script>

    <!-- NOTIF ALERT START -->
    <div class="notif cols" id="notif">
        <div class="notif-content col">
            <p>Dashword uses the revenue generated from ads to cover its costs. You are free to keep your ad blocker on, but if you want to help us continue to share high quality Geometry Dash news, we comfortably ask you to consider disabling your ad blocker on this website.</p>
        </div>
        <div>
            <button class="button" onclick="closeNotif()">Close</button>
        </div>
    </div>
    <!-- NOTIF ALERT END -->

    {% if metadata.isAds and not hideAds %}
    <!-- FIXED SLOT START -->
    <div class="fixed-slot">
        <div>
            {% include 'formats/display-wide.njk'%}
        </div>
    </div>
    <!-- FIXED SLOT END -->
    {% endif %}

    <!-- SCROLL LINK START -->
    <!--<div class="scroll" id="scrollTop" onclick="up()" onkeydown="if (event.keyCode === 13) {up()}" tabindex="0">
        <i class="ri-arrow-up-line"></i>
    </div>-->
    <!-- SCROLL LINK END -->

    <!-- MODALS START -->

    <div class="modal" id="modalTheme">
        <div class="modal-spacing">
            <div class="modal-inner">
                <form method="dialog" class="header">
                    <button onclick="closeDialog('modalTheme')" class="back" type='button'>
                        <i class="ri-close-line"></i>
                    </button>
                    <div class="label"><span>Change Theme</span></div>
                </form>
                <div class="content">
                    {# <div class="desc">
                        Choose a theme below.
                    </div> #}
                    <button onclick="setThemeDevice(); closeDialog('modalTheme');" class="button button-wide">System</button>
                    <button onclick="setThemeLight(); closeDialog('modalTheme');" class="button button-wide">Light</button>
                    <button onclick="setThemeDark(); closeDialog('modalTheme');" class="button button-wide">Dark</button>
                    {# <button onclick="closeDialog('modalTheme')" class="button button-wide">Close</button> #}
                </div>
            </div>
        </div>
    </div>

    <div class="modal" id="modalVerified">
        <div class="modal-spacing">
            <div class="modal-inner">
                <form method="dialog" class="header">
                    <button onclick="closeDialog('modalVerified')" class="back" type='button'>
                        <i class="ri-close-line"></i>
                    </button>
                    <div class="label"><span>Verified</span></div>
                </form>
                <div class="content">
                    <div class="desc">
                        {% if pageID == "authorPosts" %}
                            {% if myAuthor.staff %}
                                This author is verified because they are a Dashword staff or an official Dashword profile. <a href="/verified/">Verified Requirements</a>
                            {% elseif myAuthor.partner %}
                                This author is verified because they are partnered with Dashword. <a href="/verified/">Verified Requirements</a>
                            {% else %}
                                This author is verified because they are notable in the Geometry Dash community. <a href="/verified/">Verified Requirements</a>
                            {% endif %}
                        {% else %}
                            This should never show.
                        {% endif %}
                    </div>
                    <button onclick="closeDialog('modalVerified')" class="button button-wide">Close</button>
                </div>
            </div>
        </div>
    </div>

    <div class="modal" id="modalSearch">
        <div class="modal-spacing">
            <div class="modal-inner">
                <form method="dialog" class="header">
                    <button onclick="closeDialog('modalSearch')" class="back" type='button'>
                        <i class="ri-close-line"></i>
                    </button>
                    <div class="label"><span>Search</span></div>
                </form>
                <div class="content">
                    <div class="desc">
                        Please include something in the search field in order to search.
                    </div>
                    <button onclick="closeDialog('modalSearch')" class="button button-wide">Close</button>
                </div>
            </div>
        </div>
    </div>

    <div class="modal" id="modalSearchExt">
        <div class="modal-spacing">
            <div class="modal-inner">
                <form method="dialog" class="header">
                    <button onclick="closeDialog('modalSearchExt')" class="back" type='button'>
                        <i class="ri-close-line"></i>
                    </button>
                    <div class="label"><span>Search</span></div>
                </form>
                <div class="content">
                    {# <div class="desc">
                        Search:
                    </div> #}
                    <div class="search">
                        <form onsubmit="return false" style="margin-top: 0; flex-direction: column;">
                            <div style="display: flex; gap: 15px; align-items: center; width: 100%;">
                                <input placeholder="Search anything you want..." value="" id="searchTermMod">
                                <i class="ri-search-line"></i>
                            </div>
                            <button onclick="searchFromMod()" class="button button-wide">Search</button>
                        </form>
                    </div>
                    <div class="alert" id="searchModAlert" style="display: none !important; margin-bottom: 0;">
                        <div class="alert-icon">
                            <i class="ri-information-line"></i>
                        </div>
                        <div class="alert-content">
                            <p>Please include something in the search field in order to search.</p>
                        </div>
                    </div>
                    <script>
                        function searchFromMod() {
                            var term = document.getElementById("searchTermMod").value;
                            console.log(term);
                            if (term != '') {
                                window.location.href = "/search/?search=" + term;
                            } else {
                                console.log("Not a valid term");
                                document.getElementById("searchModAlert").style.setProperty("display", "flex", "important");
                            }
                        }
                    </script>
                </div>
            </div>
        </div>
    </div>

    <div class="modal" id="modalLeave">
        <div class="modal-spacing">
            <div class="modal-inner">
                <form method="dialog" class="header">
                    <button onclick="closeDialog('modalLeave')" class="back" type='button'>
